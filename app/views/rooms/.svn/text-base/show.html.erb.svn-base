<fieldset class="ui-widget ui-widget-content ui-corner-all">
<p>
  <b>Room number:</b>
  <%=h @room.room_number %>
</p>

<p>
  <b>Capacity:</b>
  <%=h @room.capacity %>
</p>

<p>
  <b>Building:</b>
  <%=h @room.building %>
</p>

<p>
  <b>Type:</b>
  <%=h @room.type %>
</p>
</fieldset>
<br>
<%= link_to 'Edit', edit_room_path(@room),:class => "ui-button ui-state-default ui-corner-all" %> 
<%= link_to 'Back', rooms_path,:class => "ui-button ui-state-default ui-corner-all" %>
