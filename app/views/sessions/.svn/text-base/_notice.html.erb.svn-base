<% if flash[:notice] %>
    <div id="notice" class="ui-state-highlight ui-corner-all"><p style="color: green; margin-top: 10px; padding: 0pt 0em;"><%= flash[:notice] %></p></div>
    <% flash[:notice]=nil %>
<% end %>
<% if flash[:error] %>
    <div id="errorExplanation" class="ui-state-error ui-corner-all"><p style="color: white; margin-top: 20px; padding: 0pt 0.7em;"><%= flash[:error] %></p></div>
    <% flash[:error]=nil %>
<% end %>
