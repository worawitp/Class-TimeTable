<h2 class="ui-widget-header ui-corner-all">Listing student_courses</h2>
<fieldset class="ui-widget ui-widget-content ui-corner-all">
<table>
  <tr>
    <th>Student</th>
    <th>Take course</th>
  </tr>

<% @student_courses.each do |student_course| %>
  <tr>
    <td><%=h student_course.student_id %></td>
    <td><%=h student_course.take_course_id %></td>
    <td><%= link_to 'Show', student_course,:class => "ui-button ui-state-default ui-corner-all"  %></td>
    <td><%= link_to 'Edit', edit_student_course_path(student_course),:class => "ui-button ui-state-default ui-corner-all"  %></td>
    <td><%= link_to 'Destroy', student_course, :confirm => 'Are you sure?', :method => :delete,:class => "ui-button ui-state-default ui-corner-all"  %></td>
  </tr>
<% end %>
</table>
</fieldset>
<br />

<%= link_to 'New student_course', new_student_course_path,:class => "ui-button ui-state-default ui-corner-all"  %>
