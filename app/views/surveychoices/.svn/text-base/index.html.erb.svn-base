<h2 class="ui-widget-header ui-corner-all">Listing surveychoices</h2>
<fieldset class="ui-widget ui-widget-content ui-corner-all">

<table>
  <tr>
    <th>Survey</th>
    <th>Choice number</th>
    <th>Content</th>
  </tr>

<% @surveychoices.each do |surveychoice| %>
  <tr>
    <td><%=h surveychoice.survey_id %></td>
    <td><%=h surveychoice.choice_number %></td>
    <td><%=h surveychoice.content %></td>
    <td><%= link_to 'Show', surveychoice,:class => "ui-button ui-state-default ui-corner-all" %></td>
    <td><%= link_to 'Edit', edit_surveychoice_path(surveychoice),:class => "ui-button ui-state-default ui-corner-all" %></td>
    <td><%= link_to 'Destroy', surveychoice, :confirm => 'Are you sure?', :method => :delete ,:class => "ui-button ui-state-default ui-corner-all"%></td>
  </tr>
<% end %>
</table>
<br />
</fieldset>
<%= link_to 'New surveychoice', new_surveychoice_path,:class => "ui-button ui-state-default ui-corner-all" %>
<%= link_to "Home", { :controller => "home", :action => "index" }, :class => "ui-button ui-state-default ui-corner-all"  %>
