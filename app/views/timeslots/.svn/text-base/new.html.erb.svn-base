<h1>New timeslot</h1>

<% form_for(@timeslot) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :course_id %><br />
    <%= f.text_field :course_id %>
  </p>
  <p>
    <%= f.label :day %><br />
    <%= f.text_field :day %>
  </p>
  <p>
    <%= f.label :starttime %><br />
    <%= f.time_select :starttime %>
  </p>
  <p>
    <%= f.label :endtime %><br />
    <%= f.time_select :endtime %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', timeslots_path %>